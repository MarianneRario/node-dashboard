<%for(var i=0; i < users.length;i++){%>
<tr>
  <td><%= i+1 %></td>
  <td><%= users[i].name %></td>
  <td><%= users[i].email %></td>
  <td><%= users[i].password %></td>
  <td><%= users[i].timestamp%></td>
  <td><%= users[i].udid %></td>
  <td>
    <a href="/update-user?id=<%= users[i]._id %>" class="btn boarder-shadow update">
      <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
    </a>
    <a class="btn boarder-shadow delete">
      <span class="text-gradient"><i class="fas fa-user-times"></i></span>
    </a>
  </td>
</tr>
<%}%>
